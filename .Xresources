Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

*scrollBar: false

! URxvt.font: xft:Source Code Pro for Powerline:antialias=false:size=11,xft:WenQuanYi Bitmap Song:medium:antialias=false:size=11
! URxvt.boldFont: xft:Source Code Pro for Powerline:bold:antialias=false:size=11,xft:WenQuanYi Bitmap Song:bold:antialias=false:size=11
! URxvt.font: xft:DejaVu Sans Mono:antialias=false:hinting=true:size=11,xft:WenQuanYi Bitmap Song:medium:antialias=false:size=11
! URxvt.boldFont: xft:DejaVu Sans Mono:bold:antialias=false:hinting=true:size=11,xft:WenQuanYi Bitmap Song:bold:antialias=false:size=11

URxvt.font: xft:terminus:size=12,xft:Terminess Powerline:size=12,xft:WenQuanYi Bitmap Song:medium:antialias=false:size=12,xfg:Font Awesome:hinting=true:size=12
URxvt.boldFont: xft:terminus:size=12,xft:Terminess Powerline:size=12,xft:WenQuanYi Bitmap Song:medium:antialias=false:size=12,xfg:Font Awesome:hinting=true:size=12

